    ".groupby(['STATION','dayOfWeek']).mean().sort_values('DAILY_TOTALS', ascending=False).reset_index()\n",
    "\n",
    "evening_group = evening_stations.groupby(['STATION','DATE']).sum()\\\n",
    ".groupby(['STATION','dayOfWeek']).mean().sort_values('DAILY_TOTALS', ascending=False).reset_index()\n",
    "\n",
    "\n"
